{
  "name": "Trinity Protocol",
  "entryPoints": ["src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "theme": "default",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "readme": "README.md",
  "includeVersion": true,
  "sort": ["source-order"],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "categorizeByGroup": false,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Validators", 
    "Adapters",
    "Utilities",
    "Types",
    "Constants",
    "*"
  ],
  "plugin": [],
  "treatWarningsAsErrors": false,
  "intentionallyNotExported": [
    "InternalValidator",
    "PrivateConfig"
  ],
  "requiredToBeDocumented": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludeInternal": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/canvastack/trinity/blob/main/{path}#L{line}",
  "gitRevision": "main",
  "gitRemote": "origin",
  "gaID": "",
  "hideGenerator": false,
  "searchInComments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/canvastack/trinity",
  "navigationLinks": {
    "GitHub": "https://github.com/canvastack/trinity",
    "NPM": "https://www.npmjs.com/package/@canvastack/trinity"
  },
  "customCss": "",
  "markedOptions": {},
  "blockTags": [
    "@alpha",
    "@beta", 
    "@deprecated",
    "@example",
    "@experimental",
    "@internal",
    "@override",
    "@param",
    "@privateRemarks",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@sealed",
    "@see",
    "@since",
    "@throws",
    "@virtual"
  ],
  "inlineTags": [
    "@inheritDoc",
    "@label",
    "@link",
    "@linkcode",
    "@linkplain"
  ],
  "modifierTags": [
    "@alpha",
    "@beta",
    "@deprecated", 
    "@experimental",
    "@internal",
    "@override",
    "@public",
    "@readonly",
    "@sealed",
    "@virtual"
  ]
}