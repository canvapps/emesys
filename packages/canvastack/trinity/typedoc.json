{
  "name": "Trinity Protocol",
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "theme": "default",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "README.md",
  "includeVersion": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludeInternal": true,
  "cleanOutputDir": true
}